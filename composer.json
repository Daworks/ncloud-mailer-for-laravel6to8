{
  "name": "daworks/ncloud-mailer-for-laravel6to8",
  "description": "Ncloud Cloud Outbound Mailer driver for Laravel",
  "type": "library",
  "require": {
    "php": "^7.2|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Daworks\\NcloudMailer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Daworks\\NcloudMailer\\NcloudMailerServiceProvider"
      ]
    }
  },
  "license": "MIT"
}
